<!doctype html>
<html>

<head>
    <% include head.ejs %>
</head>
<script>
    $(document).ready(function () {

    });
</script>

<body>
    <% include navbar.ejs %>
        <div class="row wrapper">
            <div class="col s12 m6 offset-m3">
                <h4 class="left-align">
                    <i class="material-icons left" style="font-size: 2.28rem">search</i>PRETRAGA</h4>
                <form id="searchPartnerForm" class="row">
                    <div class="col s12">
                        <div class="row">
                            <div class="input-field col s12">
                                <input id="companyName" type="text" class="validate" name="CompanyName">
                                <label for="companyName">Naziv kompanije</label>
                            </div>
                        </div>
                    </div>
                    <div class="col s12">
                        <div class="row center-align">
                            <button class="btn waves-effect waves-light" type="submit">PRETRAZI
                                <i class="material-icons left">search</i>
                            </button>
                        </div>
                    </div>
                </form>
                <div class="row">
                    <% for(var i=0; i < partnerPackages.length; i++) { %>
                        <a>
                            <div class="col s3 partner-package">
                                <%= partnerPackages[i] %>
                            </div>
                        </a>
                        <% } %>
                </div>
                <table class="highlight">
                    <thead>
                        <tr>
                            <th>Naziv kategorije</th>
                            <th>Naziv kompanije</th>
                            <th>Logo kompanije</th>
                            <th>Kratak opis</th>
                        </tr>
                    </thead>

                    <tbody>
                        <tr>
                            <td>Alvin</td>
                            <td>Eclair</td>
                            <td>$0.87</td>
                            <td>Opis</td>
                        </tr>
                        <tr>
                            <td>Alan</td>
                            <td>Jellybean</td>
                            <td>$3.76</td>
                            <td>Opis</td>
                        </tr>
                        <tr>
                            <td>Jonathan</td>
                            <td>Lollipop</td>
                            <td>$7.00</td>
                            <td>Opis</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <% include footer.ejs %>
</body>

</html>